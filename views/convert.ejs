<body>
  <div class="container-fluid block1">
    <div class="row">
      <div class="col-sm-12">
        Please enter doc_id: <input type="text" name="" id="doc_id">
        <button id="btn_submit">Submit</button>
        <br> <br> <br>
        <p id="show_total_page" style="display: none"></p>
        <ul id="pagination" class="pagination-sm"></ul>
      </div>
    </div>
    <p id = "err_id"> Sorry. Id not found!</p>
    <div class="row" style="height: 380px; padding: 20px">
      <div class="col-sm-4 view_image">
        <p>PNG</p>
        <img src="" style="width: 100%; height: 380px; " id="png">
      </div>
      <div class="col-sm-4 view_image">
        <p>PDF</p>
        <iframe src="" frameborder="0" style="width: 100%; height: 380px;" id="pdf"></iframe>
      </div>
      <div class="col-sm-4 view_image">
        <p>SVG</p>
        <img src="" style="width: 100%; height: 380px; " id="svg">
      </div>
      <div class="col-sm-4 view_image">
        <p>JPG</p>
        <img src="" style="width: 100%; height: 380px; " id="jpg">
      </div>
    </div>
  </div>
